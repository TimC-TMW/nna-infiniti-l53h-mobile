@import "compass";

/* mixins use to get the font you want */
@mixin default-font-n4{
  font-family:"MetaPro-n4", "MetaPro", arial, verdana, sans-serif;
  font-style:normal;
  font-weight:400;
}
@mixin default-font-i4{
  font-family:"MetaPro-i4", "MetaPro", arial, verdana, sans-serif;
  font-style:italic;
  font-weight:400;
}
@mixin default-font-n6{
  font-family:"MetaPro-n6", "MetaPro", arial, verdana, sans-serif;
  font-style:normal;
  font-weight:600;
}
@mixin default-font-i6{
  font-family:"MetaPro-i6", "MetaPro", arial, verdana, sans-serif;
  font-style:italic;
  font-weight:600;
}





/* class helpers */
.ir {
    background-color: transparent;
    border: 0;
    overflow: hidden;
    /* IE 6/7 fallback */
    *text-indent: -9999px;
}
.ir:before {
    content: "";
    display: block;
    width: 0;
    height: 100%;
}

.clearfix:before,
.clearfix:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}

.clearfix:after {
    clear: both;
}
.ruleline {
    width: 93%;
    margin: auto;
    margin-top:11px;
    margin-bottom: 11px; 
    border-bottom:1px solid #72767d;
}
.hidden {
    display: none !important;
    visibility: hidden;
}
.visuallyhidden {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
}
/*------------------------------ WEB FONTS ---------------*/
.wf-loading .wf{ visibility:hidden !important; }
.wf-inactive .wf{ visibility:visible !important; }
.wf-active .wf{ visibility:visible !important; }

