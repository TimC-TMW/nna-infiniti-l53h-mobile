<% @current_nav = 9 %> <!-- todo -->

<% content_for :pageid do %>page_colors<% end %>

<% content_for :seo do %>
<title><%= I18n.t("colors.page_title") %></title>
<meta name="description" content="<%= I18n.t("colors.page_description") %>">
<link rel="canonical" href="<%= I18n.t("colors.page_canonical") %>" />
<% end %>

<% content_for :addthis do %>
addthis:url="<%= I18n.t("colors.page_canonical") %>" addthis:description="<%= I18n.t("colors.page_description") %>" addthis:title="<%= I18n.t("colors.page_title") %>"
<% end %>

<% content_for :pagecss do %>
<% end %>

<% content_for :pagecontent do %>
<!-- page content -->
<section id="colors">
  <article id="intro" class="visuallyhidden">
    <h1><%= I18n.t("colors.header") %></h1>
  </article>
  <%= partial("partials/item_nav", :locals => { 
    :link1 => {:class => "active", :link => I18n.t("colors.item_nav1.link"), :text => I18n.t("colors.item_nav1.text")}
  }) %>
  <article>
    <div id="car_preview_container"><div id="car_preview"></div></div>
    <%= partial "partials/color_swatches" %>
    <div class="ruleline"></div>
    <%= partial "partials/cta" %>
  </article>
</section>
<% end %>

<% content_for :pagedisclaimers do %>
  <li><%= I18n.t("disclaimers.colors.exterior") %></li>
<% end %>

<% content_for :pagescript do %>
<script>
	// init page
	NNA.pageInit(function() {
		new NNA.PageControllers.Colors();
    jQuery.publish('/nna/crm/pageload');
	}); 
</script>
<% end %>